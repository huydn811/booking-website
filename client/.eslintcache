>\n                  <Form.Control\n                    type=\"text\"\n                    name=\"tourName\"\n                    placeholder=\"Name Tour\"\n                    value={values.tourName}\n                    onChange={handleChange}\n                  />\n                  <Form.Text className=\"text-muted\">\n                    {errors.tourName}\n                  </Form.Text>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicEmail\">\n                  <Form.Control\n                    type=\"text\"\n                    name=\"startAddress\"\n                    placeholder=\"Start Address\"\n                    value={values.startAddress}\n                    onChange={handleChange}\n                  />\n                  <Form.Text className=\"text-muted\">\n                    {errors.startAddress}\n                  </Form.Text>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicEmail\">\n                  <Form.Control\n                    type=\"date\"\n                    placeholder=\"Start Date\"\n                    name=\"startDate\"\n                    onChange={handleChange}\n                  />\n                  <Form.Text className=\"text-muted\">\n                    {errors.startDate}\n                  </Form.Text>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicEmail\">\n                  <Form.Control\n                    type=\"date\"\n                    placeholder=\"End Date\"\n                    name=\"endDate\"\n                    onChange={handleChange}\n                  />\n                  <Form.Text className=\"text-muted\">\n                    {errors.endDate}\n                  </Form.Text>\n                </Form.Group>\n                <Form.Group controlId=\"formBasicEmail\">\n                  <Form.Control\n                    type=\"number\"\n                    placeholder=\"Price Tour\"\n                    n